{
	"name": "n8n-nodes-outline",
	"version": "2.0.0",
	"description": "n8n community nodes for Outline knowledge base integration with complete API coverage (10 resources, 70+ operations)",
	"private": false,
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/j-shelfwood/n8n-nodes-outline.git"
	},
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"outline",
		"knowledge-base",
		"documentation",
		"notes",
		"ai-agent",
		"automation",
		"api",
		"cms",
		"wiki",
		"collaboration"
	],
	"author": "j-shelfwood",
	"license": "MIT",
	"homepage": "https://github.com/j-shelfwood/n8n-nodes-outline",
	"scripts": {
		"build": "tsc && npm run copy-files",
		"copy-files": "copyfiles -u 1 \"src/**/*.svg\" \"src/**/*.png\" dist/",
		"dev": "tsc --watch",
		"format": "prettier --write .",
		"lint": "eslint src --ext .ts",
		"lintfix": "eslint src --ext .ts --fix",
		"prepare": "npm run build",
		"prepublishOnly": "npm run lint && npm run build",
		"release:patch": "./publish.sh patch",
		"release:minor": "./publish.sh minor",
		"release:major": "./publish.sh major"
	},
	"devDependencies": {
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"copyfiles": "^2.4.1",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"prettier": "^3.2.5",
		"typescript": "^5.4.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"n8n-workflow": "^1.82.0"
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/OutlineApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Outline/Outline.node.js",
			"dist/nodes/OutlineWebhook/OutlineWebhook.node.js"
		]
	}
}
